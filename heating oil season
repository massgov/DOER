<script>
  var showNewCloud = localStorage.getItem('showNewCloud'),
      scriptTag = document.createElement('script');
  
  if (showNewCloud) scriptTag.src = '/highcharts.cloud.v0.0.1.dist.js';
  else scriptTag.src = '/old.highcharts.cloud.v0.0.1.dist.js';
  
  document.body.appendChild(scriptTag);
</script>

